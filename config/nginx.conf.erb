daemon off;

worker_processes <%= ENV['NGINX_WORKERS'] || 4 %>;

events {
    use epoll;
    accept_mutex on;
    worker_connections <%= ENV['NGINX_WORKER_CONNECTIONS'] || 1024 %>;
}

http {
    server {
        listen <%= ENV["PORT"] %>;

        location / {
            proxy_pass http://0.0.0.0:5000;
        }
    }
}